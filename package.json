		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-pluginutils": "^2.8.2",
		"shx": "^0.3.2",
		"typescript": "^4.0.3",
		"webrain": "^1.2.2-a"
	},
	"dependencies": {
		"caller": "^1.0.1",
		"postcss-js-syntax": "^1.0.7",
		"resolve-from": "^5.0.0"
	},
	"peerDependencies": {
		"postcss": ">=7.0.0"
	}
}
